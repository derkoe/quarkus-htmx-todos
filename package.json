{
  "devDependencies": {
    "husky": "6.0.0",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "prettier-plugin-java": "1.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{json,yml,yaml,java}": [
      "prettier --write",
      "git add"
    ]
  }
}
